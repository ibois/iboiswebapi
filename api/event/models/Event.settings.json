{
  "connection": "default",
  "collectionName": "event",
  "info": {
    "name": "event",
    "description": ""
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "minLength": 4
    },
    "decription": {
      "type": "text"
    },
    "starts_at": {
      "type": "date",
      "required": true
    },
    "website": {
      "type": "string",
      "minLength": 4,
      "required": true
    },
    "users": {
      "collection": "user",
      "via": "lanparties",
      "plugin": "users-permissions"
    },
    "ends_at": {
      "type": "date",
      "required": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    }
  }
}